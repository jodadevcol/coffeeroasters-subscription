{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@data/*": [
        "data/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@icons/*": [
        "icons/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@sections/*": [
        "sections/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
    }
  }
}